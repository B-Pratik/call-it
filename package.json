{
  "name": "call-it",
  "version": "1.0.0",
  "description": "peer-to-peer video calling app",
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build-local": "webpack",
    "watch": "webpack-dev-server",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/B-Pratik/call-it.git"
  },
  "author": "Pratik Bhurewar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/B-Pratik/call-it/issues"
  },
  "homepage": "https://github.com/B-Pratik/call-it#readme",
  "browserslist": {
    "development": [
      "last 1 version",
      "not IE 11",
      "not IE_Mob 11"
    ],
    "production": [
      "last 1 version",
      "not IE 11",
      "not IE_Mob 11"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "Promise": true
    },
    "extends": [
      "react",
      "prettier",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "prettier",
      "react-hooks"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "peer": "^0.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "antd-mobile": "^2.3.3",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.2",
    "mini-css-extract-plugin": "^0.11.0",
    "peerjs": "^1.3.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
